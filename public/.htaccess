<IfModule mod_rewrite.c>
	Options -Multiviews
	RewriteEngine On
	RewriteCond %{SERVER_PORT} 80
        RewriteRule ^(.*)$ https://www.dominio.com/$1 [R,L]
	RewriteBase /
	RewriteCond %{REQUEST_FILENAME} !-d
	RewriteCond %{REQUEST_FILENAME} !-f
	RewriteRule ^(.+)$ index.php?url=$1 [QSA,L]
</IfModule>
